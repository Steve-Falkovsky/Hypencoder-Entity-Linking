{
    "ngrams": {
        "ngram_range": [3, 3],
        "max_features": 50000,
        "min_df": 10,
        "char_side": "front",
        "char_count": 16
    },
    
    "crossencoder": {
        "use_top_k": 5,
        "learning_rate": 5e-6,
        "batch_size": 8,
        "onto_mode": "name+aliases+semantic_types+broader_concepts+definition"
    },
    
    "multicrossencoder": {
        "top_k": 5,
        "learning_rate": 1e-6,
        "batch_size": 2
    },
    
    "mention_detector": {
        "learning_rate": 2e-5,
        "batch_size": 8,
        "weight_decay": 0.0,
        "mask_neg_rate": 0.0
    }
}
